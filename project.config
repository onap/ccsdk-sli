[access]
	inheritFrom = All-Projects
[project]
	description = Contains code for Service Logic Interpreter (SLI)
[access "refs/*"]
	owner = group saml/onap-gerrit-ccsdk-sli-core-committers
	forgeCommitter = group saml/onap-gerrit-ccsdk-sli-core-committers
[access "refs/for/refs/heads/*"]
	push = +force group self-serve-release
	pushMerge = group self-serve-release
[plugin "its-jira"]
	enabled = false
